{
    "name": "arete/specification",
    "type": "library",
    "license": "MIT",
    "version": "0.3.4",
    "description": "PHP Specification library.",
    "keywords": ["specification", "ddd", "satisfy", "satisfied"],
    "authors": [
        {
            "name": "AreteCode",
            "email": "aretecode@gmail.com"
        }
    ],
    "require": {
        "arete/support": "dev-master",
        "arete/collection-pipeline": "dev-master"
    },       
    "require-dev": {            
        "phpunit/phpunit": "5.1.*"    
    },
    "autoload": {
        "psr-4": {
            "Arete\\Specification\\": ["src/", "tests/"]
        }    
    },        
    "autoload-dev": {
        "psr-4": {
            "Examples\\": "examples/"
        }    
    },        
    "config": {
        "bin-dir": "bin/"
    },    
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.4-dev"
        }
    },
    "minimum-stability": "dev"
}
